+++ b/src/game/object/SettingState.java	(date 1761743141255)
@@ -0,0 +1,79 @@
+package game.state;
+
+import game.manager.GameStateManager;
+
+import javax.swing.*;
+import java.awt.*;
+import java.awt.event.MouseEvent;
+import java.awt.event.MouseListener;
+
+public class SettingState implements GameState, MouseListener {
+    private final JPanel panel;
+    private final GameStateManager gsm;
+    private final PlayState playState;
+
+    private Image bg;
+    private Rectangle newGameBtn, quitBtn, closeBtn;
+
+    public SettingState(JPanel panel, GameStateManager gsm, PlayState playState) {
+        this.panel = panel;
+        this.gsm = gsm;
+        this.playState = playState;
+
+        bg = new ImageIcon(getClass().getResource("/image/SettingMenu.png")).getImage();
+
+        newGameBtn = new Rectangle(150, 100, 200, 50);
+        quitBtn = new Rectangle(150, 180, 200, 50);
+        closeBtn = new Rectangle(150, 260, 200, 50);
+
+        panel.addMouseListener(this);
+    }
+
+    @Override
+    public void update() {}
+
+    @Override
+    public void draw(Graphics g) {
+        g.drawImage(bg, 0, 0, 400, 400, null);
+
+        g.setFont(new Font("Arial", Font.BOLD, 24));
+        g.setColor(Color.WHITE);
+        g.drawString("New Game", newGameBtn.x + 20, newGameBtn.y + 35);
+        g.drawString("Quit to Menu", quitBtn.x + 20, quitBtn.y + 35);
+        g.drawString("Close", closeBtn.x + 20, closeBtn.y + 35);
+    }
+
+    @Override
+    public void keyPressed(int keyCode) {}
+    @Override
+    public void keyReleased(int keyCode) {}
+
+    @Override
+    public void mouseClicked(MouseEvent e) {
+        Point p = e.getPoint();
+        if (newGameBtn.contains(p)) {
+            // SỬA: Tạo PlayState mới thay vì restart
+            gsm.setStates(new PlayState(panel, gsm));
+            cleanup();
+        } else if (quitBtn.contains(p)) {
+            gsm.setStates(new MenuState(panel, gsm));
+            cleanup();
+        } else if (closeBtn.contains(p)) {
+            gsm.pop(); // SỬA: Dùng pop để quay lại PlayState
+            cleanup();
+        }
+    }
+
+    private void cleanup() {
+        panel.removeMouseListener(this);
+    }
+
+    @Override
+    public void mousePressed(MouseEvent e) {}
+    @Override
+    public void mouseReleased(MouseEvent e) {}
+    @Override
+    public void mouseEntered(MouseEvent e) {}
+    @Override
+    public void mouseExited(MouseEvent e) {}
+}
